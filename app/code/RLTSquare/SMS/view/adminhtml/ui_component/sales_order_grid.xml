<?xml version="1.0"?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <listingToolbar name="listing_top">
        <massaction name="listing_massaction" class="RLTSquare\SMS\Ui\Component\MassAction\Listing">
            <action name="bulk_sms_send">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="type" xsi:type="string">bulk_sms_send</item>
                        <item name="label" xsi:type="string" translate="true">Bulk Sms Resend</item>
                        <item name="url" xsi:type="url" path="rltsms/index/massSms"/>
                    </item>
                </argument>
            </action>
        </massaction>
    </listingToolbar>
</listing>
