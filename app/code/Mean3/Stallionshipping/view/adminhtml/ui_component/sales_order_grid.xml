<?xml version="1.0" encoding="UTF-8"?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
        <listingToolbar name="listing_top">
        <massaction name="listing_massaction">
            <action name="book">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="type" xsi:type="string">Book Through Stallion</item>
                        <item name="label" xsi:type="string" translate="true">Book Through Stallion</item>
                        <item name="url" xsi:type="url" path="stallionshipping/index/massbook" />
                        <item name="confirm" xsi:type="array">
                            <item name="title" xsi:type="string" translate="true">Book Through Stallion</item>
                            <item name="message" xsi:type="string" translate="true">Are you sure you wan't to book selected order(s)?</item>
                        </item>
                        <item name="spinner" xsi:type="string">SalesOrderGrid</item>
                    </item>
                </argument>
            </action>
            <action name="stallion_print_more">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="type" xsi:type="string">print stallion label</item> 
                        <item name="label" xsi:type="string" translate="true">Print stallion label</item> 
                        <item name="url" xsi:type="url" path="stallionshipping/order/massPrintMore" /> 
                    </item>
                </argument>
            </action>

        </massaction>
    </listingToolbar>
</listing>